#include<stdio.h>
void main()
{
    int n,i,j;
    printf("enter n value: ");
    scanf("%d",&n);
    int r,c;
    for(r=1;r<=n;r++)
    {
        for(c=1;c<=n;c++)
        {
            printf("%d\t",r*c);
        }
        printf("\n");
    }
  
}

output-----
enter n value: 5
1	2	3	4	5	
2	4	6	8	10	
3	6	9	12	15	
4	8	12	16	20	
5	10	15	20	25	



